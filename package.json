{
  "devDependencies": {
    "concurrently": "^4.1.0",
    "jest": "^24.8.0",
    "md5": "^2.2.1",
    "mongoose": "^5.5.4",
    "nay": "^1.0.2",
    "prettier": "^1.17.1"
  },
  "peerDependencies": {
    "mongoose": "^5.5.4"
  },
  "scripts": {
    "test": "concurrently --raw --kill-others --success first \"TZ=Etc/UTC jest --watch\" \"mongod --quiet | nay\"",
    "test:debug":  "concurrently --raw --kill-others --success first \"TZ=Etc/UTC node --inspect-brk node_modules/.bin/jest --runInBand\" \"mongod --quiet | nay\"",
    "test:only": "TZ=Etc/UTC jest"
  }
}
